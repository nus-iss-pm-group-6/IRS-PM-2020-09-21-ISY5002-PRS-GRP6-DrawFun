# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Space indentation
[*]
indent_style = space
trim_trailing_whitespace = true
insert_final_newline = true

# Matches the exact file package.json
[{package.json, *.yml, *.yaml, *.vue}]
indent_style = space
indent_size = 2
